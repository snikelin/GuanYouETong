body
  font-family 'SimSun',Tahoma
  font-size 12px

.page-header
  font-size 11px
  margin 10px 20px
.page-footer
  margin 0 20px

.title
  text-align center
  margin-top 30px
  h1
    font-family "Arial Black"
    font-size 18px
    margin 5px

.table-left
  float left
  width 15px
  font-size 13px
  margin-top 20px
  writing-mode vertical-rl
  -webkit-writing-mode vertical-rl
.table-main
  margin-left 15px
  padding-left 5px
  padding-right 10px
  table, tr, td, th
    border 1px solid black
    table-layout fixed
  table
    margin 5px 0
    border-collapse collapse
    width 100%
    background-image url("./background.png")
    background-repeat no-repeat
    background-position center
    th.no
      width 8%
    th.name
      width 45%
      letter-spacing 1em
    th.amount
      width 15%
    th.price
      width 10%
    th.percentage
      width 8%
    th
      height 30px
      text-align: center
    td
      height 23px
      vertical-align middle
      text-align center
    td.name
      text-align left

    td.footer
      height 40px
      padding-left 10px
      text-align left

.stamp
  position absolute
  width 218px
  height 212px
  top 200px
  right 30px
  background-image url("./stamp.png")